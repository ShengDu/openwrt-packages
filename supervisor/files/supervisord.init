#!/bin/sh /etc/rc.common

START=96
export PATH=/bin:/sbin:/usr/bin:/usr/sbin:/mnt/storage/usr/bin
APP=supervisord
PID_FILE=/var/run/$APP.pid

start() {
        start-stop-daemon -S -x $APP -p $PID_FILE -m -b
}

stop() {
        start-stop-daemon -K -n $APP -p $PID_FILE -s TERM
        rm -rf $PID_FILE
}
